{
  "extends": [
    "development"
  ],
  "hints": {
    "no-inline-styles": "off",
    "axe/name-role-value": "off",
    "axe/text-alternatives": [
      "default",
      {
        "image-alt": "off",
        "frame-title": "off"
      }
    ],
    "meta-viewport": "off",
    "compat-api/html": [
      "default",
      {
        "ignore": [
          "iframe[allow]"
        ]
      }
    ],
    "button-type": "off",
    "axe/aria": "off",
    "axe/keyboard": "off"
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not and_chr <= 116",
    "not chrome <= 116",
    "not edge <= 116",
    "not samsung <= 21"
  ]
}